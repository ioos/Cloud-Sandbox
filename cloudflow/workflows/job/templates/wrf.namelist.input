 j_parent_start                      = 1,       49,      30,
 parent_grid_ratio                   = 1,       3,       3,
 parent_time_step_ratio              = 1,       3,       3,
 feedback                            = 1,    ! 0: no feeback, 1: two-way
 smooth_option                       = 0     ! 0: no, smoothing parent domain aroud nest area
 smooth_option                       = 0
